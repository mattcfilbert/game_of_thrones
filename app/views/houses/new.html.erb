<h2>New House</h2>


<% @house.errors.full_messages.each do |message| %>
  <p><%= message %></p>
<% end %>

<%= form_for @house do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :img_url %>
  <%= f.text_field :img_url %>

  <%= f.label :motto %>
  <%= f.text_field :motto %>

  <%= f.submit %>
<% end %>
